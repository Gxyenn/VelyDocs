{
  "api": {
    "version": "2.0.0",
    "baseUrl": "https://api.velydocs.com",
    "rateLimit": {
      "default": {
        "requests": 60,
        "window": "1m",
        "message": "Rate limit exceeded. Please try again later."
      },
      "premium": {
        "requests": 300,
        "window": "1m",
        "message": "Premium rate limit exceeded."
      },
      "unlimited": {
        "requests": -1,
        "window": "1m",
        "message": "No limit"
      }
    }
  },
  "specialAgents": {
    "enabled": true,
    "keys": [
      {
        "key": "VELY_AGENT_MASTER_001",
        "name": "Master Agent",
        "tier": "unlimited",
        "active": true,
        "createdAt": "2024-01-01T00:00:00.000Z"
      },
      {
        "key": "VELY_AGENT_DEV_002",
        "name": "Development Agent",
        "tier": "premium",
        "active": true,
        "createdAt": "2024-01-01T00:00:00.000Z"
      }
    ]
  },
  "sources": {
    "winbu": {
      "baseUrl": "https://winbu.net",
      "enabled": true,
      "endpoints": ["ongoing", "latest", "anime", "search", "genre"]
    },
    "samehadaku": {
      "baseUrl": "https://v1.samehadaku.how",
      "enabled": true,
      "endpoints": ["ongoing", "anime", "search", "schedule", "genre", "batch"]
    },
    "kuramanime": {
      "baseUrl": "https://v12.kuramanime.tel",
      "enabled": true,
      "endpoints": ["ongoing", "anime", "search", "schedule", "genre", "latest"]
    },
    "otakudesu": {
      "baseUrl": "https://otakudesu.cloud",
      "enabled": true,
      "endpoints": ["ongoing", "anime", "search", "schedule", "genre", "complete", "batch"]
    }
  },
  "features": {
    "cache": {
      "enabled": false,
      "ttl": 300
    },
    "cors": {
      "enabled": true,
      "origins": ["*"]
    },
    "logging": {
      "enabled": true,
      "level": "info"
    }
  }
}
